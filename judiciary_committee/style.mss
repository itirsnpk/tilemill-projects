#-atlanticarticlegunma {
  [zoom<=3] { marker-width:9; }
  [zoom=4]  { marker-width:10; }
  [zoom=5]  { marker-width:11; }
  [zoom=6]  { marker-width:12; }
  [zoom>=7] { marker-width:13; }
  marker-line-width: 20;
  marker-line-color: transparent;
  marker-fill:#f60923;
  marker-allow-overlap:true;
}
